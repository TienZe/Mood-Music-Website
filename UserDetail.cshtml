@{
    Layout = "_Layout";

}

@model List<AppUser>


    <div class="main_contents_userDetail">
        <div class="main_userDetail">
            <h1 class="header_userDetail">Member list</h1>
            <div class="userdetail_table">
            <table class="userdetail_list">
               
                <thead>
                    <tr>
                        <th>STT</th>
                        <th>Username</th>
                        <th>Account</th>
                        <th>Cart</th>
                        <th>Register day</th>
                        <th>Action</th>
                    </tr>
                </thead>
                
                <tbody>
                @foreach(var i in this.Model)
                {
                    //int j = 0;
                    <tr>
                        <td>@i.Id</td>
                        <td>@i.Name</td>
                        <td>@i.Email</td>
                        <td>Unknown</td>
                        <td>Unknown</td>
                        <td><a asp-controller="Admin" asp-action="Detail" asp-route-Id="@i.Id">Go to profile detail</a></td>
                            
                        
                    </tr>    
                }
            </tbody>

            </table>
        </div>
            @*<div class="contents_bar_userDetail">

            </div>*@
        </div>
    </div>